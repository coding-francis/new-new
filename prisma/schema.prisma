generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource postgresql {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id    BigInt     @id @default(autoincrement()) @map("id")
  email String  @unique
  name  String?
  password String

  @@map("users")
}
